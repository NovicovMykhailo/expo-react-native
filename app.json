{
  "expo": {
    "name": "AndroidApp",
    "slug": "AndroidApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow AndroidApp to access your photos.",
          "savePhotosPermission": "Allow AndroidApp to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow AndroidApp to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow AndroidApp to save photos."
      }
    },
    "android": {
      "package": "com.myself.phonygram",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBuntfGXvYboVRWyzXjUUtOBCtBRgas3eg"
        }
      },
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "ACCESS_BACKGROUND_LOCATION"
        ]
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./src/assets/icon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "39c3fe85-9e03-48d4-969f-a121dcc1f6b2"
      }
    }
  }
}
